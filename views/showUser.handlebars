<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    <th scope="col">PHone</th>
    <th scope="col">City</th>
    <th scope="col"><a href="/home" class="btn btn-primary">Add User</a></th>
    </tr>
  </thead>
  <tbody>
    {{#each list}}
    <tr class="table-active">
      <td>{{this.name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td>{{this.city}}</td>
      <td><a href="/upd/{{this.id}}" class="btn btn-info">Edit</a></td>
            <td><a href="/del/{{this.id}}" class="btn btn-danger">Delete</a></td>
      </tr>
    {{/each}}
      </tbody>
      </table>